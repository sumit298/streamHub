{
  "name": "ils-backend",
  "version": "1.0.0",
  "description": "Interactive Live Streaming Backend",
  "main": "server.js",
  "scripts": {
    "start": "node --env-file=.env server.js",
    "dev": "node --env-file=.env.local --watch server.js",
    "seed": "node seed-data.js",
    "test:basic": "node test-basic.js",
    "test:services": "node test-services.js",
    "test:api": "node test-api.js",
    "test:all": "npm run test:basic && npm run test:services",
    "worker": "node --env-file=.env.local src/workers/vodWorker.js"

  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.989.0",
    "@aws-sdk/s3-request-presigner": "^3.989.0",
    "amqplib": "^0.10.3",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.2.1",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mediasoup": "^3.12.0",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "prom-client": "^14.2.0",
    "sanitize-html": "^2.17.0",
    "socket.io": "^4.7.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "axios": "^1.5.0",
    "socket.io-client": "^4.8.1"
  }
}
